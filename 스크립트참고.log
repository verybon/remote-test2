            (function(){
                var ic18btn = document.querySelectorAll('.ic18 button');
                var allbtn = document.querySelector('.button-answer.ic18');
                var allbtnText = allbtn.querySelector('.text');
                allbtn.dataset.on = 'off';

                for(var i=0; i < ic18btn.length; i++) {
                    ic18btn[i].addEventListener('click', function(){
                        console.log(this.dataset.n);
                        var n = this.dataset.n;
                        var c8 = document.querySelector('.n' + n);
                        c8.classList.add('on');
                        c8.dataset.on = 'on';
                        check();
                    });
                }

                function check(){
                    var ic18span= document.querySelectorAll('.ic182 span');
                    var all = 0;

                    for(var j=0; j < ic18span.length; j++) {
                        if(ic18span[j].dataset.on === 'on') {
                            all = all + 1;
                        }
                        console.log(all)
                        if (all === ic18span.length) {
                            allbtn.dataset.on = 'on';
                            allbtn.classList.add('on');
                            allbtnText.innerText = '정답 가리기';
                        } else {
                            allbtn.dataset.on = 'off';
                            allbtn.classList.remove('on');
                            allbtnText.innerText = '정답 확인';
                        }
                    }
                }

                function checkToggle(v){
                    var ic18span= document.querySelectorAll('.ic182 span');

                    for(var j=0; j < ic18span.length; j++) {
                        if(v) {
                            ic18span[j].classList.add('on');
                            ic18span[j].dataset.on = 'on';
                            allbtn.dataset.on = 'on';
                            allbtn.classList.add('on');
                            allbtnText.innerText = '정답 가리기';

                        }else {
                            ic18span[j].classList.remove('on');
                            ic18span[j].dataset.on = 'off';
                            allbtn.dataset.on = 'off';
                            allbtn.classList.remove('on');
                            allbtnText.innerText = '정답 확인';
                        }

                        // if (all === ic18span.length) {
                        //     allbtn.dataset.on = 'on';
                        //     allbtn.classList.add('on');
                        //     allbtnText.innerText = '정답 가리기';
                        // } else {
                        //     allbtn.dataset.on = 'off';
                        //     allbtn.classList.remove('on');
                        //     allbtnText.innerText = '정답 확인';
                        // }
                    }
                }

                allbtn.addEventListener('click', function(){
                    console.log(this.dataset.n);
                    var n = this.dataset.n;
                    var c8 = document.querySelector('.n' + n);

                    if (allbtn.dataset.on === 'off') {
                        checkToggle(true)
                    } else {
                        checkToggle(false)
                    }
                    //c8.classList.add('on');
                });
            })();


